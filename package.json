{
	"name": "chat",
	"version": "1.0.0",
	"main": "./src/index.ts",
	"private": true,
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "node -r ts-node/register ./src/index.ts",
		"ext": "*.ts, *.js"
	},
	"scripts": {
		"dev": "nodemon",
		"build": "rollup -c",
		"start": "node ./dist/index.js"
	},
	"author": "daviprios",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^23.0.2",
		"@rollup/plugin-json": "^5.0.1",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^9.0.2",
		"@types/node": "^18.11.9",
		"nodemon": "^2.0.20",
		"rollup": "^3.2.5",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"chalk": "4",
		"socket.io": "^4.5.3"
	}
}
